{
  "manifest_version": 3,
  "name": "Vimium New Tab Page",
  "version": "0.1",
  "description": "Show a new tab page that Vimium can control",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "options.html",
  "chrome_url_overrides": { "newtab": "redirect.html" },
  "permissions": ["tabs", "storage"]
}
